<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="BuyBoxProduct" strict="true">
        <property name="stockQuantity" type="float"/>
        <property name="isNeverOutOfStock" type="bool"/>
        <property name="price" type="CurrentProductPrice"/>
        <property name="merchant" type="MerchantStorage"/>
        <property name="merchantReference" type="string"/>
        <property name="idMerchant" type="int"/>
        <property name="productOfferReference" type="string"/>
        <property name="inventoryStatus" type="string"/>
        <property name="isAvailable" type="bool"/>
        <property name="baseUnit" type="ProductMeasurementUnit"/>
    </transfer>

    <transfer name="ProductOfferStorageCriteria">
        <property name="productConcreteSkus" type="string[]" singular="productConcreteSku"/>
    </transfer>

    <transfer name="ProductView">
        <property name="idProductAbstract" type="int"/>
        <property name="idProductConcrete" type="int"/>
        <property name="sku" type="string"/>
        <property name="stockQuantity" type="float" strict="true"/>
        <property name="isNeverOutOfStock" type="bool" strict="true"/>
        <property name="baseUnit" type="ProductMeasurementUnit" strict="true"/>
        <property name="available" type="bool"/>
        <property name="currentProductPrice" type="CurrentProductPrice"/>
        <property name="productOfferReference" type="string"/>
    </transfer>

    <transfer name="CurrentProductPrice">
    </transfer>

    <transfer name="MerchantStorageCriteria">
        <property name="merchantReferences" type="string[]" singular="merchantReference"/>
    </transfer>

    <transfer name="PriceProductFilter">
        <property name="quantity" type="int"/>
    </transfer>

    <transfer name="MerchantStorage">
        <property name="idMerchant" type="int"/>
        <property name="urlCollection" type="Url[]" singular="url"/>
    </transfer>

    <transfer name="Url">
    </transfer>

    <transfer name="ProductMeasurementUnit">
    </transfer>

</transfers>
